{
  "name": "visorcaceres",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --extra-webpack-config webpack.partial.js -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --exclude src/app/modules/users/**",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "docs:live": "compodoc -p src/tsconfig.app.json -d docs -s --watch",
    "lint:ci": "npm run lint && npm run lint:styles",
    "lint:styles": "stylelint ./src/**/*.scss",
    "prepush": "npm run lint:ci",
    "mocksserver": "node mocks_server/run.js",
    "proxy": "node proxy/proxy.js",
    "build_des": "ng build --configuration=desa",
    "build_pre": "ng build --configuration=preprodu",
    "build_pro": "ng build --configuration=production",
    "build_all": "npm run build_des && npm run build_pre && npm run build_pro",
    "deploy_des": "npm run build_des && node deploys/deploy_des",
    "deploy_des_mocks": "node deploys/deploy_des_mocks"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.15",
    "@angular/compiler": "~7.2.15",
    "@angular/core": "~7.2.15",
    "@angular/forms": "~7.2.15",
    "@angular/platform-browser": "~7.2.15",
    "@angular/platform-browser-dynamic": "~7.2.15",
    "@angular/router": "~7.2.15",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/openlayers": "4.6.16",
    "browser-xml2js": "^0.4.19",
    "buffer": "^5.2.1",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.2",
    "jsts": "^2.3.0",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.3.1",
    "ngx-build-plus": "^7.8.3",
    "ngx-logger": "^3.3.12",
    "openlayers": "4.6.5",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.3",
    "proj4": "^2.4.4",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.4.0",
    "rxjs-tslint": "^0.1.7",
    "shp-write": "^0.3.2",
    "shpjs": "^3.4.3",
    "stylelint": "^9.10.1",
    "togpx": "^0.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.9",
    "@juancarlosrmr/node-sftp-deploy-sp": "^1.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angularx-social-login": "^1.2.7",
    "codelyzer": "~4.5.0",
    "cors-anywhere": "^0.4.1",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jwt-decode": "^2.2.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mockserver": "^3.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
