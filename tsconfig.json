{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@theme/*": [
          "src/app/modules/theme/*"
        ],
      "@geospatial/*": [
          "src/app/modules/geospatial/*"
        ],
      "@cotvisor/*": [
          "src/app/modules/@cotvisor/*"
        ],
      "@cotvisor-admin/*": [
          "src/app/modules/@cotvisor-admin/*"
        ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "types": ["node"],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
